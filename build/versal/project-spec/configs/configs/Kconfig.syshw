menu "Memory Settings"
choice
	prompt "Primary Memory"
	help
	  The configuration in this menu impacts the memory settings in the device tree
	  autoconfig files.
	  If you select 'manual', PetaLinux will auto generate memory node based on user inputs,
	  you will need to specify base address and memory size.
	  To skip generating lower or upper memory node specify 0x0 offset to the memory size.
	config SUBSYSTEM_MEMORY_C0_DDR_LOW0_BANKLESS_SELECT
	bool "C0_DDR_LOW0"
config SUBSYSTEM_MEMORY_C0_DDR_LOW1_BANKLESS_SELECT
	bool "C0_DDR_LOW1"
config SUBSYSTEM_MEMORY_C0_DDR_CH1X2_BANKLESS_SELECT
	bool "C0_DDR_CH1x2"

config SUBSYSTEM_MEMORY_MANUAL_SELECT
	bool "manual"

	endchoice


config SUBSYSTEM_MEMORY_C0_DDR_LOW0_BANKLESS_BASEADDR
	hex "System memory base address"
	default 0x00000000
	range 0x00000000 0x7e000000
	depends on SUBSYSTEM_MEMORY_C0_DDR_LOW0_BANKLESS_SELECT
	help
	  Start address of the system memory.
	  It has to be within the selected primary memory physical address range.


config SUBSYSTEM_MEMORY_C0_DDR_LOW1_BANKLESS_BASEADDR
	hex "System memory base address"
	default 0x800000000
	range 0x800000000 0x97e000000
	depends on SUBSYSTEM_MEMORY_C0_DDR_LOW1_BANKLESS_SELECT
	help
	  Start address of the system memory.
	  It has to be within the selected primary memory physical address range.


config SUBSYSTEM_MEMORY_C0_DDR_CH1X2_BANKLESS_BASEADDR
	hex "System memory base address"
	default 0x50000000000
	range 0x50000000000 0x501fe000000
	depends on SUBSYSTEM_MEMORY_C0_DDR_CH1X2_BANKLESS_SELECT
	help
	  Start address of the system memory.
	  It has to be within the selected primary memory physical address range.


config SUBSYSTEM_MEMORY_MANUAL_LOWER_BASEADDR
	hex "Lower memory base address"
	default 0x0
	depends on SUBSYSTEM_MEMORY_MANUAL_SELECT
	help
	  base address of the lower memory
	  Make sure the DT memory entry should start with provided address.


config SUBSYSTEM_MEMORY_C0_DDR_LOW0_BANKLESS_SIZE
	hex "System memory size"
	default 0x80000000
	range 0x2000000 0x80000000
	depends on SUBSYSTEM_MEMORY_C0_DDR_LOW0_BANKLESS_SELECT
	help
	  Size of the system memory. Minimum is 32MB, maximum is the size of
	  the selected primary memory physical address range.


config SUBSYSTEM_MEMORY_C0_DDR_LOW1_BANKLESS_SIZE
	hex "System memory size"
	default 0x180000000
	range 0x2000000 0x180000000
	depends on SUBSYSTEM_MEMORY_C0_DDR_LOW1_BANKLESS_SELECT
	help
	  Size of the system memory. Minimum is 32MB, maximum is the size of
	  the selected primary memory physical address range.


config SUBSYSTEM_MEMORY_C0_DDR_CH1X2_BANKLESS_SIZE
	hex "System memory size"
	default 0x200000000
	range 0x2000000 0x200000000
	depends on SUBSYSTEM_MEMORY_C0_DDR_CH1X2_BANKLESS_SELECT
	help
	  Size of the system memory. Minimum is 32MB, maximum is the size of
	  the selected primary memory physical address range.


config SUBSYSTEM_MEMORY_MANUAL_LOWER_MEMORYSIZE
	hex "Lower memory size"
	default 0x80000000
	depends on SUBSYSTEM_MEMORY_MANUAL_SELECT
	help
	  Size of the lower memory. Minimum is 32MB, maximum is the size of
	  the selected primary memory physical address range.
	  If you specify 0x0 offset then it will skip generating lower memory node.


config SUBSYSTEM_MEMORY_MANUAL_UPPER_BASEADDR
	hex "Upper memory base address"
	default 0x800000000
	depends on SUBSYSTEM_MEMORY_MANUAL_SELECT
	depends on SUBSYSTEM_ARCH_AARCH64
	help
	  base address of the upper memory
	  Make sure the DT memory entry should start with provided address.


config SUBSYSTEM_MEMORY_MANUAL_UPPER_MEMORYSIZE
	hex "Upper memory size"
	default 0x80000000
	depends on SUBSYSTEM_MEMORY_MANUAL_SELECT
	depends on SUBSYSTEM_ARCH_AARCH64
	help
	  Size of the upper memory. Minimum is 32MB, maximum is the size of
	  the selected primary memory physical address range.
	  If you specify 0x0 offset then it will skip generating upper memory node.


config SUBSYSTEM_MEMORY_C0_DDR_LOW0_BANKLESS_U__BOOT_TEXTBASE_OFFSET
	hex "u-boot text base address offset to memory base address"
	default 0x8000000 if SUBSYSTEM_ARCH_AARCH64
	default 0x4000000 if SUBSYSTEM_ARCH_ARM
	default 0x100000 if SUBSYSTEM_ARCH_MICROBLAZE
	range 0x100000 0x7e100000
	depends on SUBSYSTEM_MEMORY_C0_DDR_LOW0_BANKLESS_SELECT
	depends on !SUBSYSTEM_COMPONENT_U__BOOT_NAME_NONE


config SUBSYSTEM_MEMORY_C0_DDR_LOW1_BANKLESS_U__BOOT_TEXTBASE_OFFSET
	hex "u-boot text base address offset to memory base address"
	default 0x808000000 if SUBSYSTEM_ARCH_AARCH64
	default 0x804000000 if SUBSYSTEM_ARCH_ARM
	default 0x800100000 if SUBSYSTEM_ARCH_MICROBLAZE
	range 0x800100000 0x97e100000
	depends on SUBSYSTEM_MEMORY_C0_DDR_LOW1_BANKLESS_SELECT
	depends on !SUBSYSTEM_COMPONENT_U__BOOT_NAME_NONE


config SUBSYSTEM_MEMORY_C0_DDR_CH1X2_BANKLESS_U__BOOT_TEXTBASE_OFFSET
	hex "u-boot text base address offset to memory base address"
	default 0x50008000000 if SUBSYSTEM_ARCH_AARCH64
	default 0x50004000000 if SUBSYSTEM_ARCH_ARM
	default 0x50000100000 if SUBSYSTEM_ARCH_MICROBLAZE
	range 0x50000100000 0x501fe100000
	depends on SUBSYSTEM_MEMORY_C0_DDR_CH1X2_BANKLESS_SELECT
	depends on !SUBSYSTEM_COMPONENT_U__BOOT_NAME_NONE



config SUBSYSTEM_MEMORY_IP_NAME
	string
	default C0_DDR_LOW0
	depends on SUBSYSTEM_MEMORY_C0_DDR_LOW0_BANKLESS_SELECT


config SUBSYSTEM_MEMORY_IP_NAME
	string
	default C0_DDR_LOW1
	depends on SUBSYSTEM_MEMORY_C0_DDR_LOW1_BANKLESS_SELECT


config SUBSYSTEM_MEMORY_IP_NAME
	string
	default C0_DDR_CH1X2
	depends on SUBSYSTEM_MEMORY_C0_DDR_CH1X2_BANKLESS_SELECT


endmenu

