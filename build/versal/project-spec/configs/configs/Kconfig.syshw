	string "Static IP gateway"
	default "192.168.0.1"
	depends on SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_SELECT && !SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_USE_DHCP
