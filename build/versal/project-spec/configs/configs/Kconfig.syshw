menu "Ethernet Settings"
choice
	prompt "Primary Ethernet"
	help
	  Select a Ethernet used as primary Ethernet.
	  The primary ethernet will be used for u-boot networking if u-boot is
	  selected and will be used as eth0 in Linux.
	  If your preferred primary ethernet is not on the list, please select
	  'manual'.
config SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_SELECT
	bool "CIPS_0_pspmc_0_psv_ethernet_0"
config SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_1_SELECT
	bool "CIPS_0_pspmc_0_psv_ethernet_1"
config SUBSYSTEM_ETHERNET_MANUAL_SELECT
	bool "manual"

endchoice


config SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_MAC_AUTO
	bool "Randomise MAC address"
	default y if SUBSYSTEM_ARCH_MICROBLAZE
	default n
	depends on SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_SELECT
	help
	  randomise MAC address for the primary ethernet.

config SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_MAC_PATTERN
	string "Template for randomised MAC address"
	default "00:0a:35:00:??:??"
	depends on SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_SELECT && SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_MAC_AUTO
	help
	  Pattern for generating random MAC addresses - question mark
	  characters will be replaced by random hex digits

config SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_MAC
	string "Ethernet MAC address"
	default "ff:ff:ff:ff:ff:ff"
	depends on SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_SELECT && !SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_MAC_AUTO
	help
	  Default mac set to ff:ff:ff:ff:ff:ff invalid mac address to read from EEPROM
	  if you want change with desired value you can change, example: 00:0a:35:00:22:01

config SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_1_MAC_AUTO
	bool "Randomise MAC address"
	default y if SUBSYSTEM_ARCH_MICROBLAZE
	default n
	depends on SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_1_SELECT
	help
	  randomise MAC address for the primary ethernet.

config SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_1_MAC_PATTERN
	string "Template for randomised MAC address"
	default "00:0a:35:00:??:??"
	depends on SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_1_SELECT && SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_1_MAC_AUTO
	help
	  Pattern for generating random MAC addresses - question mark
	  characters will be replaced by random hex digits

config SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_1_MAC
	string "Ethernet MAC address"
	default "ff:ff:ff:ff:ff:ff"
	depends on SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_1_SELECT && !SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_1_MAC_AUTO
	help
	  Default mac set to ff:ff:ff:ff:ff:ff invalid mac address to read from EEPROM
	  if you want change with desired value you can change, example: 00:0a:35:00:22:01


config SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_USE_DHCP
	bool "Obtain IP address automatically"
	default y
	depends on SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_SELECT
	help
	  Set this option if you would like your SUBSYSTEM to use DHCP for
	  obtaining an IP address.

config SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_IP_ADDRESS
	string "Static IP address"
	default "192.168.0.10"
	depends on SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_SELECT && !SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_USE_DHCP
	help
	  The IP address of your main network interface when static network
	  address assignment is used.

config SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_IP_NETMASK
	string "Static IP netmask"
	default "255.255.255.0"
	depends on SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_SELECT && !SUBSYSTEM_ETHERNET_CIPS_0_PSPMC_0_PSV_ETHERNET_0_USE_DHCP
	help
	  Default netmask when static network address assignment is used.
	  In case of systemd please specify netmask value like CIDR notation Eg: 24 instead of 255.255.255.0
	  In case of sysvinit please specify netmask value like dot-decimal notation Eg: 255.255.255.0 instead of 24 

